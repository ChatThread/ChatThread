# Example environment variables for local development / open-source builds.
# Copy to `.env.local` (NOT committed) and fill values as needed.

# Backend API base URL
VITE_API_BASE_URL=http://localhost:7860

# Optional: public website base URL (for Terms/Privacy links)
VITE_PUBLIC_SITE_URL=

# Optional: Vite public base path (usually '/')
VITE_BASE_URL=/

# Optional: Aliyun OSS upload (only used in production builds when all values are set)
VITE_OSS_DIST=/chatthread/
VITE_OSS_REGION=
VITE_OSS_ACCESS_KEY_ID=
VITE_OSS_ACCESS_KEY_SECRET=
VITE_OSS_BUCKET=
VITE_OSS_OVERWRITE=false
VITE_OSS_PRINT_RAW=false
